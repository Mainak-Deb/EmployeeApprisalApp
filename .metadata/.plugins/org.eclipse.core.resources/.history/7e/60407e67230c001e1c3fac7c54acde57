<struts-config>


  <action-mappings>
    <action path="/login" type="com.employee.action.LoginAction" name="LoginAction"
     scope="request"  >
      <forward name="admin" path="/login.do" />
      <forward name="user" path="/WEB-INF/views/createUser.jsp" />
    </action>
  </action-mappings>
  
  
  <action-mappings>
    <action path="/createUserPage" type="com.student.action.CreateUserPage"
      name="CreateUserPage">
      <forward name="success" path="/WEB-INF/views/createUser.jsp" />
    </action>
  </action-mappings>
  
 </struts-config>
 